{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 500,
    "height": 300,
    "data": {
      "url": "https://raw.githubusercontent.com/stefan-ty-su/fit3179/refs/heads/main/homework5/data/STE_2021_AUST_GDA2020.json",
      "format": {
        "type": "topojson",
        "feature": "STE_2021_AUST_GDA2020"
      }
    },
    "transform": [{
      "lookup": "properties.STE_NAME21",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/stefan-ty-su/fit3179/refs/heads/main/homework5/data/hw5.csv",
          "format": {
            "type": "csv"
          }
        },
        "key": "State",
        "fields": ["Persons", "Total", "Country of Birth"]
      }
    },
    {
        "calculate": "(datum.Persons/datum.Total) * 100",
        "as": "% Australian Born"
    }],
    "projection": {
      "type": "mercator"
    },
    "mark": {
        "type": "geoshape",
        "stroke": "white"
    },
    "encoding": {
      "color": {
        "field": "% Australian Born",
        "type": "quantitative"
      },
      "tooltip": [
        {"field": "properties.STE_NAME21", "title": "State"},
        {"field": "Proportion", "title": "% Australian Born", "format": ".2f"}
      ]
    }
}